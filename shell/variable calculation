temp_b="$(cat temps/region_B)‚Äù   #There is only one number in the file
temp_c="$(cat temps/region_C)"

# Create an array with these variables as elements region_temps=($temp_b $temp_c)

# Call an external program to get average temperature average_temp=$(echo "scale=2; (${region_temps[0]} + ${region_temps[1]}) / 2" | bc)

# Append average temp to the array
region_temps+=($average_temp)

# Print out the whole array
echo ${region_temps[@]}
